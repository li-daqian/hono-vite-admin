{
  "name": "hono-vite-admin",
  "private": true,
  "packageManager": "pnpm@10.27.0",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "prisma:reset": "pnpm --filter @hono-vite-admin/server prisma:reset",
    "openapi-ts": "pnpm --filter @hono-vite-admin/admin openapi-ts",
    "shadcn": "pnpm --filter @hono-vite-admin/admin shadcn"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.7.3",
    "typescript": "5.9.3"
  }
}
